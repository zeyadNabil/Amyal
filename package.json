{
  "name": "amyal-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate:partners && ng serve",
    "start:mobile": "npm run generate:partners && ng serve --host 0.0.0.0",
    "start:watch": "npm run generate:partners && concurrently \"npm run watch:partners\" \"ng serve\"",
    "start:watch:mobile": "npm run generate:partners && concurrently \"npm run watch:partners\" \"ng serve --host 0.0.0.0\"",
    "build": "npm run generate:partners && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:partners": "node scripts/generate-partner-images.js",
    "watch:partners": "node scripts/watch-partner-images.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "bootstrap": "^5.3.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
